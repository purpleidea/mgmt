import "fmt"

$some_list = ["l", "m", "n",]

$some_map = {
	"ottawa" => 6,
	"toronto" => 7,
	"montreal" => 8,
	"vancouver" => 9,
}

print "letter" {
	msg => fmt.printf("letter: %s", $some_list[1]),

	Meta:autogroup => false,
}

print "city" {
	msg => fmt.printf("city: %d", $some_map["montreal"]),

	Meta:autogroup => false,
}
