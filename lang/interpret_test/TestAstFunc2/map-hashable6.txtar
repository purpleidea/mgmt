-- main.mcl --

$st0 = struct{foo => "a", bar => ["a1", "a2",],}
$st1 = struct{foo => "a", bar => ["a1", "a2",],}
$st2 = struct{foo => "b", bar => ["b1", "b2", "b3",],}

$m = {$st1 => "pass", $st2 => "fail",}

$s = $m[$st0] || "error"

test "${s}" {}
